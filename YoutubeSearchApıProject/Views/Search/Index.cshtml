<body>
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">

                <a class="navbar-brand" href="./">
                    <img class="img-logo" src="data:image/png;base64,">
                </a>
            </div>
            <div class="collapse navbar-collapse" id="myNavbar">

                <form class="navbar-form navbar-left form-search hidden-xs" method="get">
                    <div class="input-group">
                        <input type="text" class="form-control form-search search-ip" placeholder="search" name="search" />
                        <div class="input-group-btn">
                            <button class="btn btn-default btn-search" type="submit"><i class="fa fa-search"></i></button>
                        </div>
                    </div>
                </form>

            </div>
        </div>
    </nav>

    <div class="content">
        <div class="content hssa m-left">
            <div class="container">
                <div class="row">

                    <div class="col-md-10">
                        <div class="ads-bg-top">
                            <a target="_blank" href="#">
                                <img class="img-responsive" width="970" height="250" src="http://placehold.it/970x250">
                            </a>
                        </div>
                        <div class="title-topic">
                            <span>Search Results</span>
                        </div>

                        <ul class="list-unstyled video-list-thumbs row">

                            @foreach (var item in Model)
                            {
                                <li class="col-lg-3 col-sm-4 col-xs-6">
                                    <a href="#" title="@item.VideoDescription">
                                        <a href="@item.VideoUrl"><img src="@item.ImgUrl" alt="Barca" class="img-responsive" height="130px" /> </a>
                                        <h2>@item.VideoTitle</h2>
                                        <i class="fa fa-play-circle-o"></i>
                                        @*<span class="duration">03:15</span>
                                    <span class="time">1 phút trước</span>*@
                                    </a>
                                </li>
                            }



                        </ul>

                    </div>
                </div>
            </div>
        </div>
    </div>

    <script>
        (function ($) {
            $(window).on("load", function () {
                $(".my-sideber").mCustomScrollbar({ theme: "dark-3" });
            });
        })(jQuery);
    </script>
</body>
</html>